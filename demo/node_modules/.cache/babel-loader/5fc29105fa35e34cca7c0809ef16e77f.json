CA,WAAA,WAAA;AACA,KAhBA;AAiBA;AACA,IAAA,WAlBA,yBAkBA;AACA,UAAA,MAAA,GAAA,KAAA,SAAA,CAAA,KAAA;AACA,UAAA,OAAA,GAAA,KAAA,SAAA,CAAA,OAAA;AACA,UAAA,QAAA,GAAA,KAAA,SAAA,CAAA,QAAA;AACA,UAAA,KAAA,GAAA,CAAA,OAAA,GAAA,CAAA,IAAA,QAAA;AACA,UAAA,IAAA,GAAA,CAAA,OAAA,GAAA,CAAA,IAAA,QAAA,GAAA,QAAA;AACA,UAAA,IAAA,GAAA,MAAA,GAAA,CAAA,EAAA,IAAA,GAAA,MAAA;AACA,WAAA,UAAA,GAAA,KAAA,WAAA,CAAA,KAAA,CAAA,KAAA,EAAA,IAAA,CAAA;AACA,KA1BA;AA2BA;AACA,IAAA,QA5BA,oBA4BA,KA5BA,EA4BA;AACA,MAAA,MAAA,CAAA,cAAA,CAAA,OAAA,CAAA,OAAA,EAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,WAAA;AACA,KA/BA;AAgCA,IAAA,cAhCA,0BAgCA,KAhCA,EAgCA,OAhCA,EAgCA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,iBAAA,EAAA;AAAA,kBAAA,KAAA,EAAA,KAAA;AAAA,kBAAA,OAAA,EAAA;AAAA,iBAAA,CADA;;AAAA;AAAA;AACA,gBAAA,MADA,yBACA,IADA;;AAAA,sBAEA,MAAA,IAAA,OAFA;AAAA;AAAA;AAAA;;AAAA,kDAEA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA;;AAHA;AAAA,uBAIA,MAAA,CAAA,MAAA,EAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAKA;AArCA;AAlBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"main\" v-for=\"item in myQuestion\" :key=\"item._id\">\r\n      <div class=\"item\" @click=\"question(item.title)\">\r\n         <h2>{{item.title}}</h2>\r\n        <span class=\"iconfont icon-user\"></span>:{{item.writer}}\r\n        <span class=\"iconfont icon-danju\"></span>:{{item.date}}\r\n        <br />\r\n        <span class=\"iconfont icon-showpassword\"></span>:{{item.describe}}\r\n        <el-button class=\"delete\" type=\"warning\" @click.stop=\"questionDelete(item.title,item.pagenum)\">删除</el-button>\r\n      </div>\r\n    </div>\r\n    <!--分页区-->\r\n    <el-pagination\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"queryInfo.pagenum\"\r\n      :page-sizes=\"[1, 2, 3, 5]\"\r\n      :page-size=\"queryInfo.pagesize\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"queryInfo.total\"\r\n    ></el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      questionAll: [],\r\n      myQuestion: [],\r\n      object: {\r\n        name: window.sessionStorage.getItem(\"name\")\r\n      },\r\n      queryInfo: {\r\n        pagenum: 1,\r\n        pagesize: 3,\r\n        total: 0\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.getall();\r\n  },\r\n  methods: {\r\n    async getall() {\r\n      const result = await this.$http.post(\"/myquestion\", this.object);\r\n      this.queryInfo.total = result.data.length;\r\n      this.questionAll = result.data;\r\n      this.getThisPage();\r\n    },\r\n    //监听 pagesize改变的事件（总页数）每页有多少个元素\r\n    handleSizeChange(newsize) {\r\n      this.queryInfo.pagesize = newsize;\r\n      this.getThisPage();\r\n    },\r\n    //监听 页码值改变的事件（当前页的改变）第几页\r\n    handleCurrentChange(newpage) {\r\n      this.queryInfo.pagenum = newpage;\r\n      this.getThisPage();\r\n    },\r\n    //分页：获取当前页的数据\r\n    getThisPage() {\r\n      var length = this.queryInfo.total;\r\n      var pagenum = this.queryInfo.pagenum;\r\n      var pagesize = this.queryInfo.pagesize;\r\n      var first = (pagenum - 1) * pagesize;\r\n      var last = (pagenum - 1) * pagesize + pagesize;\r\n      if (last > length - 1) last = length;\r\n      this.myQuestion = this.questionAll.slice(first, last);\r\n    },\r\n    //跳转到问题的具体网页\r\n    question(title) {\r\n      window.sessionStorage.setItem(\"title\", title);\r\n      this.$router.push(\"/question\");\r\n    },\r\n    async questionDelete(title,pagenum) {\r\n      const {data:result}=await this.$http.post(\"/questionDelete\",{title:title,pagenum:pagenum});\r\n      if(result==\"error\")return this.$message.error(\"删除失败\");\r\n      this.$message.success(\"黑历史被抹除了！\");\r\n      await this.getall();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.item {\r\n  border: 2px solid rgb(255, 79, 143);\r\n  margin-bottom: 10px;\r\n  border-radius: 5px;\r\n  overflow: hidden;\r\n  white-space: nowrap;\r\n  text-overflow: ellipsis;\r\n  position: relative;\r\n  .delete {\r\n    position: absolute;\r\n    top: 10px;\r\n    right: 20px;\r\n    width: 50px;\r\n    height: 30px;\r\n    text-align: center;\r\n    padding: 0;\r\n  }\r\n}\r\n.item:hover {\r\n  background-color: rgba(0, 0, 0, 0.02);\r\n  cursor: pointer;\r\n  box-shadow: -2px -2px 7px -2px #000;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}